
<h1>Homepage</h1>
<div class="row">
  <% for(var i=0;i<usuarios.length;i++){ %>
  <div class="col-sm-4">

    <h1><%=usuarios[i].nombres%></h1>

    <form action="/Usuario/eliminarUsuario" method="post">

      <input hidden
             type="text"
             value="<%=usuarios[i].id%>"
             name="id">

      <button  type="submit"
        class="btn btn-outline-danger">Borrar</button>

    </form>


  </div>
  <%}%>

</div>















